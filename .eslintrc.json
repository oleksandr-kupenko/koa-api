{
  "env": {
    "es2021": true,
    "browser": true
  },
  "extends": ["eslint:recommended", "airbnb-base"],
  "parserOptions": {
    "ecmaVersion": 12,
    "sourceType": "module"
  },
  "rules": {
    "linebreak-style": 0,
    "no-console": "off",
    "prefer-destructuring": "off",
    "comma-dangle": "off"
  }
}
